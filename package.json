{
  "name": "koa-react-boilerplate",
  "version": "1.0.0",
  "description": "Template project for web application.",
  "main": "index.js",
  "scripts": {
    "start": "env NODE_ENV=production node server/index.js",
    "build": "rimraf dist && env NODE_ENV=production webpack --config ./webpack.production.config.js",
    "eslint": "eslint .",
    "jscs": "jscs .",
    "dev-back": "nodemon -w server -w config server/index.js",
    "dev-front": "webpack-dev-server",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register --require babel-polyfill",
    "test-cover": "NODE_ENV=test babel-node ./node_modules/istanbul/lib/cli cover _mocha",
    "test-watch": "npm test -- --watch"
  },
  "author": "essem",
  "license": "ISC",
  "dependencies": {
    "co-body": "^4.2.0",
    "config": "^1.21.0",
    "isomorphic-fetch": "^2.2.1",
    "koa": "^1.2.1",
    "koa-cors": "0.0.16",
    "koa-morgan": "=0.x",
    "koa-route": "^2.4.2",
    "koa-send": "^3.2.0",
    "koa-static": "^2.0.0",
    "mkdirp": "^0.5.1",
    "winston": "^2.2.0",
    "winston-daily-rotate-file": "^1.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "bootstrap": "^3.3.7",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "css-loader": "^0.23.1",
    "deep-freeze": "0.0.1",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "istanbul": "^1.0.0-alpha",
    "jscs": "^3.0.7",
    "mocha": "^3.0.2",
    "nodemon": "^1.10.0",
    "react": "^15.3.0",
    "react-bootstrap": "^0.30.2",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-devtools": "^3.3.1",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
